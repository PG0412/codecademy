      "source": [
        "def fib(num):\n",
        "  if num==1:\n",
        "    return 1\n",
        "  elif num==2:\n",
        "    return 2\n",
        "  else:\n",
        "    return fib(num-1)+fib(num-2)\n",
        "\n",
        "count=0\n",
        "x=2\n",
        "num=2 #Starting from 2 as 1 is odd\n",
        "while num<=4000000:\n",
        "  if num%2==0:\n",
        "    count+=num\n",
        "  x+=1\n",
        "  num=fib(x)\n",
        "\n",
        "print(count)"
      ]
    }
